<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :stars %>
  <%= f.text_field :stars %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit %>

<% end %>
